{
    "name": "Axelfvm/laravelcarrinhocompras",
    "description": "Laravel carrinho de compras",
    "type": "library",
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Axelfvm\\LaravelCarrinhoCompras\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Axelfvm\\LaravelCarrinhoCompras\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Axelfvm\\LaravelCarrinhoCompras\\Facades\\Cart"
            }
        }
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
